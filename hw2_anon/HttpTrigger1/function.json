{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "dataType": "binary",
      "path": "requestsstorage/{name}_{datetime:yyyy-mm-dd-hh-mm-ss}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
